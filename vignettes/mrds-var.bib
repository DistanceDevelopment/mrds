@article{conn_accommodating_2013,
  title = {Accommodating Species Identification Errors in Transect Surveys},
  author = {Conn, Paul B. and McClintock, Brett T. and Cameron, Michael F. and Johnson, Devin S. and Moreland, Erin E. and Boveng, Peter L.},
