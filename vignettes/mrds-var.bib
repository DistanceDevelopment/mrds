@article{pace_state-space_2017,
  title = {State-Space Mark-Recapture Estimates Reveal a Recent Decline in Abundance of {{North Atlantic}} Right Whales},
  author = {Pace, Richard M. and Corkeron, Peter J. and Kraus, Scott D.},
  year = {2017},
