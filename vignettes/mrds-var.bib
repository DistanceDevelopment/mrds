@article{moore_regional_2011,
  title = {The {{Regional Ocean Modeling System}} ({{ROMS}}) 4-Dimensional Variational Data Assimilation Systems},
  author = {Moore, Andrew M. and Arango, Hernan G. and Broquet, Gregoire and Edwards, Chris and Veneziani, Milena and Powell, Brian and Foley, Dave and Doyle, James D. and Costa, Dan and Robinson, Patrick},
