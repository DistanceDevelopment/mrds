@article{moretti_risk_2014,
  title = {A {{Risk Function}} for {{Behavioral Disruption}} of {{Blainville}}'s {{Beaked Whales}} ({{Mesoplodon}} Densirostris) from {{Mid-Frequency Active Sonar}}},
  author = {Moretti, David and Thomas, Len and Marques, Tiago and Harwood, John and Dilley, Ashley and Neales, Bert and Shaffer, Jessica and McCarthy, Elena and New, Leslie and Jarvis, Susan and Morrissey, Ronald},
