\name{errors}
\alias{errors}
\title{Error function}
\usage{
  errors(errmsg = NULL, errmode = "report",
    preamble = "Warning")
}
\arguments{
  \item{errmsg}{the message to be stored/printed
  (optional)}

  \item{errmode}{report or print errors (default report)}

  \item{preamble}{character string to paste before the
  message}
}
\value{
  None
}
\description{
  Writes error messages for various errors that can occur
}
\author{
  Dave Miller
}

