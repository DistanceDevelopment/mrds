\name{gof.rem}
\alias{gof.rem}
\title{Compute chi-square gof test for rem models}
\usage{
gof.rem(model, breaks = NULL, nc = NULL)
}
\arguments{
  \item{model}{ddf model object}

  \item{breaks}{distance cut points}

  \item{nc}{number of distance classes}
}
\value{
list with chi-square value, df and p-value
}
\description{
Compute chi-square gof test for rem models
}

